#include "support/langs.h"

#include "memory/memory.h"

@EXTERNS@

langs_t get_langs(void)
{
    const language_t kLangArray[] = { @MODS@ };
    size_t len = sizeof(kLangArray) / sizeof(language_t);

    langs_t ret = {
        .langs = ctu_memdup(kLangArray, sizeof(kLangArray)),
        .size = len
    };
    return ret;
}
