def take0(ptr: int): int {
    return ptr;
}

def take1(ptr: *int): int {
    return *ptr;
}

def take3(ptr: ***int): int {
    return ***ptr;
}

def test(): void {
    final i = 20;
    final $ = take1(&i);

    final a = 0;
    final b = &a;
    final c = &b;
    final d = &c;

    take3(d);

    final a1 = 0;
    final a2 = &a1;
    final a3 = &a2;
    final a4 = &a3;

    take0(***a4);

    // final $ = take(&100);
}
